{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "plugin:react/jsx-runtime", "prettier", "@react-native-community"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "react/no-multi-comp": 0,
    "react/no-deprecated": 1,
    "react/jsx-key": 1,
    "react/jsx-no-undef": 1,
    "react/prop-types": 0,
    "semi": [1, "never"],
    "react/no-unstable-nested-components": 0,
    "react-native/no-inline-styles": 0
  }
}
